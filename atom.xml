---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Coding Fearlessly</title>
 <link href="http://codingfearlessly.com/atom.html" rel="self"/>
 <link href="http://codingfearlessly.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://codingfearlessly.com/</id>
 <author>
   <name>Mindaugas Mozūras</name>
   <email>mindaugas.mozuras@gmail.com</email>
 </author>

 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://codingfearlessly.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://codingfearlessly.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
   <author>
     <name>Mindaugas Mozūras</name>
     <uri>http://codingfearlessly.com/</uri>
   </author>
 </entry>
 {% endfor %}

</feed>
